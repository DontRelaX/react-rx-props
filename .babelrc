{
  "presets": [
    "react",
    [
      "es2015",
      {
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "es2015"
      ],
      "plugins": [
        "transform-class-properties"
      ]
    }
  }
}